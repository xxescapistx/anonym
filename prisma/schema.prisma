generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

model posts {
  id Int @unique @id @default(autoincrement())
  user String @db.VarChar(40)
  message String @db.VarChar()
  tags String @db.VarChar()
  datetime DateTime @default(now())
}